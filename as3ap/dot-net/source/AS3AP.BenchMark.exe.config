<?xml version="1.0" encoding="utf-8" ?>
<configuration>
<appSettings>
	<add key="UserNumber" value="0" />
	<!--
		relation size (tuples)		-	logical database size (bytes)
			10.000					-	4 megabytes 
			100.000					-	40 megabytes
			1.000.000				-	400 megabytes
			10.000.000				-	4 gigabytes
			100.000.000				-	40 gigabytes
	-->	
	<add key="DataSize" value="10000" />
	
	<!--
		Data creation method allow two values:
			
			LOAD - Will load data from data files located in the path specified in
					DataPath key.
					
			CREATE - Withh create data on the fly.
		
	-->
	<add key="RunCreate" value="true" />
	<add key="DataCreationMethod" value="LOAD" />
	<add key="DataPath" value="D:\DATA-4MB" />	
		
	<add key="Backend" value="AS3AP.BenchMark.Backends.FirebirdSql" />	
	<add key="ConnectionString" value="Database=D:\AS3AP.FDB;User=SYSDBA;Password=masterkey;Server=localhost;Port=3050;Dialect=3;Charset=NONE;Role=;Connection Lifetime=15;Pooling=true" />
	
	<!--
		Sequence of tests to run with the next format:
			TestType:TestType;TestType:TestType;TestType:TestType
		
		Each pair of TestType:TestType defines a sequence.
		
		TestType can be ( non case sensitive ):
			
			- SQL87. Run the subsequent tests using SQL87 sintax.
			- SQL92. Run the subsequent tests using SQL92 sintax.
			- singleuser. Run singleuser tests.
			- multiuser.  Run multiuser tests.
	-->
	<add key="RunSequence" value="SQL87;singleuser;SQL92;singleuser" />
	
	<!--	Configuration for Neutral backend
	<add key="BackendAssembly" value="FirebirdSql.Data.Firebird" />
	<add key="BackendConnectionClass" value="FirebirdSql.Data.Firebird.FbConnection" />
	<add key="BackendCommandClass" value="FirebirdSql.Data.Firebird.FbCommand" />
	<add key="BackendParameterClass" value="FirebirdSql.Data.Firebird.FbParameter" />
	-->		
</appSettings>
</configuration>
